{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " \uf133 {{ .CurrentDate | date \"2006-01-02\" }} | \uf017 {{ .CurrentDate | date \"03:04:05 pm\" }} ",
          "foreground": "#4A5759",
          "background": "#FAA307",
          "type": "time",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "template": " {{.Icon}} ",
          "foreground": "#4A5759",
          "background": "#D9822B",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uf007 {{.UserName}} ",
          "foreground": "#FDF0D5",
          "background": "#6A7B76",
          "type": "session",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if .Root }} \uf0e7 {{ end }}",
          "foreground": "#111111",
          "background": "#FFB703",
          "type": "session",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "full"
          },
          "template": " \ue5ff {{ .Path }} ",
          "foreground": "#FDF0D5",
          "powerline_symbol": "\ue0b0",
          "background": "#4A5759",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_max_length": 25,
            "cache_duration": "none",
            "display_untracked_files": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " \uf126 {{ .HEAD }}{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}{{ if gt .Working.Modified 0 }} \uf044 {{ .Working.Modified }}{{ end }}{{ if gt .Working.Added 0 }} \uf067 {{ .Working.Added }}{{ end }}{{ if gt .Working.Untracked 0 }} \uf067 {{ .Working.Untracked }}{{ end }}{{ if gt .Working.Deleted 0 }} \uf1f8 {{ .Working.Deleted }}{{ end }}{{ end }}{{ if and .Working.Changed .Staging.Changed }} |{{ end }}{{ if .Staging.Changed }}{{ if gt .Staging.Added 0 }} \uf055 {{ .Staging.Added }}{{ end }}{{ if gt .Staging.Modified 0 }} \uf044 {{ .Staging.Modified }}{{ end }}{{ if gt .Staging.Deleted 0 }} \uf1f8 {{ .Staging.Deleted }}{{ end }}{{ end }}{{ if gt .StashCount 0 }} \uf187 {{ .StashCount }}{{ end }} ",
          "foreground": "#FDF0D5",
          "powerline_symbol": "\ue0b0",
          "background": "#A4161A",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FAA307{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#E85D04{{ end }}",
            "{{ if gt .Ahead 0 }}#A4161A{{ end }}",
            "{{ if gt .Behind 0 }}#A4161A{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue718 {{ .Full }} ",
          "foreground": "#FDF0D5",
          "powerline_symbol": "\ue0b0",
          "background": "#90A555",
          "type": "node",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "extensions": ["angular.json", "nx.json", "package.json"]
          },
          "template": " \ue753 {{ .Full }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#DD0031",
          "type": "angular",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue77c {{ .Full }} ",
          "foreground": "#FDF0D5",
          "powerline_symbol": "\ue0b0",
          "background": "#E85D04",
          "type": "dotnet",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "austin",
            "threshold": 500
          },
          "template": " \uf252 {{ .FormattedMs }} ",
          "foreground": "#FDF0D5",
          "powerline_symbol": "\ue0b0",
          "background": "#4A5759",
          "type": "executiontime",
          "style": "powerline"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "{{ if gt .Code 0 }}\uf00d{{ else }}\uf00c{{ end }}",
          "foreground": "#90A955",
          "type": "status",
          "style": "plain",
          "foreground_templates": ["{{ if gt .Code 0 }}#BA181B{{ end }}"]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " ‚ùØ ",
          "foreground": "#D9822B",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
